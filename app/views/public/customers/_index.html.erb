<table class='table'>
  <thead>
    <tr>
      <th>プロフィール画像</th>
      <th>ニックネーム</th>
      <th colspan="3"></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% customers.each do |customer| %>
      <tr>
        <td><%= image_tag customer.get_profile_image(70,70) %></td>
        <td><%= customer.nickname %></td>
        <td></td>
        <td><%= link_to '詳細', customer_path(customer) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
